  User sgeneves
  ProxyCommand ssh g5k "nc -q 0 `basename %h .g5k` %p"
  ForwardAgent no


# Automatic connection to hosts within Grid'5000, and not reachable direction on SSH tcp port 22
Host *.g5k
  User sgeneves
  ProxyCommand ssh g5k "nc -q 0 `basename %h .g5k` %p"
  ForwardAgent no

